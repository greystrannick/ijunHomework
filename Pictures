using System;

namespace CsLight
{
    class Program
    {
        static void Main(string[] args)
        {
            int numberOfImages = 52;
            int numberOfCompletedRows = 3;
            int resultRow;
            int resultRemainder;

            resultRow = numberOfImages / numberOfCompletedRows;
            resultRemainder = numberOfImages % numberOfCompletedRows;
            Console.WriteLine($"Количество рядов: {resultRow}, количество картинок сверх меры: {resultRemainder}.");
        }
    }
}
